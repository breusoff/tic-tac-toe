{
    "name": "tic-tac-toe",
    "version": "1.1.0",
    "homepage": "https://breusoff.github.io/tic-tac-toe",
    "private": true,
    "author": {
        "name": "Vladimir Breusov",
        "email": "breusov.vova@gmail.com",
        "url": "https://github.com/breusoff"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx,css,scss}'",
        "prepare": "husky install",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "release-major": "standard-version --release-as major",
        "release-minor": "standard-version --release-as minor",
        "release-patch": "standard-version --release-as patch"
    },
    "dependencies": {
        "@reduxjs/toolkit": "1.8.6",
        "i18next": "21.9.2",
        "i18next-browser-languagedetector": "6.1.5",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-i18next": "11.18.6",
        "react-modal": "3.15.1",
        "react-range": "1.8.14",
        "react-redux": "8.0.5",
        "react-router-dom": "6.4.2",
        "react-scripts": "5.0.1",
        "react-select": "5.4.0",
        "styled-components": "5.3.6",
        "typescript": "4.8.4"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.3",
        "@commitlint/config-conventional": "16.2.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "13.5.0",
        "@trivago/prettier-plugin-sort-imports": "3.3.0",
        "@types/jest": "27.5.2",
        "@types/node": "16.11.64",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "@types/react-modal": "3.13.1",
        "@types/styled-components": "5.1.26",
        "create-react-component-folder": "0.3.7",
        "eslint": "7.32.0",
        "eslint-config-airbnb-typescript-prettier": "4.2.0",
        "eslint-plugin-only-warn": "1.0.2",
        "gh-pages": "^4.0.0",
        "husky": "7.0.4",
        "lint-staged": "12.1.4",
        "prettier": "2.5.1",
        "standard-version": "9.3.2"
    },
    "crcf": [
        "stylesext",
        "notest",
        "typescript",
        "uppercase",
        "functional",
        {
            "output": "src/components"
        }
    ],
    "lint-staged": {
        "src/**/*.(js|jsx|ts|tsx|css|scss)": [
            "prettier --write"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
